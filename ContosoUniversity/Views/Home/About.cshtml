@model IEnumerable<ContosoUniversity.Models.SchoolViewModels.RegistrationDateGroup>

    @{ 
        ViewData["Title"] = "Student Body Statistics";
    }

<h2>Student Body Statistics</h2>

<table>
    <tr>
        <th>
            Registration Date
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>